package final1;

public class Main {
//   Main 클래스
//   - 사용자가 메뉴를 통해 게시물을 작성, 삭제, 조회할 수 있어야 한다
   public static void main(String[] args) {
	Board b = new Board();
	b.addPost("된다고 해주라", "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ");
	b.addPost("된거야?", "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ");
	b.addPost("된거아니지..?", "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ");
	b.deletePost(0);
	b.deletePost(0);
	b.getAllposts();
   }
}
